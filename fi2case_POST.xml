<fi2case xmlns="http://www.fi2.se/schemas/1.31">
    <fi2case_ids></fi2case_ids>
    <fi2case_childobject fi2item="fi2order">
        <fi2child_item>
            <fi2order>
                <fi2order_ids></fi2order_ids>
                <!--
                    Åtgärdsprioritet med tre nivåer enligt klasslista Class_Fi2OrderPriorityClass_OpenArchetype.xml
                       1 = Hög
                       2 = Normal
                       3 = Låg
                -->
                <fi2order_class>
                    <fi2class_code>1</fi2class_code>
                    <fi2class_scheme>
                        <fi2scheme_id>Class_Fi2OrderPriorityClass_01</fi2scheme_id>
                        <fi2scheme_name>Class_Fi2OrderPriority</fi2scheme_name>
                        <fi2scheme_url>https://github.com/FAST2/iot-case-xml/blob/master/classlist/Class_Fi2OrderPriorityClass_OpenArchetype.xml</fi2scheme_url>
                    </fi2class_scheme>
                </fi2order_class>
                <!-- Fritext som beskriver felet -->
                <fi2order_descr lang="sv" usage="Description">Här kommer en fritextbeskrivning av felet med alla detaljer. Kan vara vad som helst.</fi2order_descr>
                <!-- Kopplingen till fi2equipment/utrustning. Motsvarar en enhet (FAST2), detalj (Vitec) eller liknande i fastighetssystemet.
                    Kopplingen till det obligatoriska fi2spatisystem görs baserat på placeringen av fi2equipment
                     ID:t är löpnummer för utrustningen/detaljen i fastighetssystemet -->
                <fi2order_parentobject fi2item="fi2equipment">
                    <fi2parent_ids>
                        <fi2_id>12345</fi2_id>
                    </fi2parent_ids>
                </fi2order_parentobject>
            </fi2order>
        </fi2child_item>
    </fi2case_childobject>
</fi2case>